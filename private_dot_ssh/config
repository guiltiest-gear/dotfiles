Host *
  Compression yes
  # Use a shared channel for multiple sessions of the same host
  # It doesn't make sense to duplicate sessions anyway
  # This should lead to improved connection times
  ControlMaster auto
  ControlPath ~/.ssh/control/%r@%h:%p
  ControlPersist 600
Host aur.archlinux.org
  IdentityFile ~/.ssh/aur
  User aur
Host pi.hole
  IdentityFile ~/.ssh/dietpi
  User dietpi
Host codeberg.org
  IdentityFile ~/.ssh/codeberg
  User git
  ControlPersist 120
Host github.com
  IdentityFile ~/.ssh/github
  User git
  ControlPersist 120
